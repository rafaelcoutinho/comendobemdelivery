<queue-entries>
	<queue>
		<name>longLiveMailer</name>
		<rate>1/m</rate>
		<retry-parameters>
			<min-backoff-seconds>90</min-backoff-seconds>
			<task-retry-limit>2</task-retry-limit>
			<task-age-limit>6h</task-age-limit>
		</retry-parameters>		
	</queue>	
	<queue>
		<name>mailer</name>
		<rate>1/s</rate>
		<retry-parameters>
		<task-age-limit>12h</task-age-limit>
			<min-backoff-seconds>10</min-backoff-seconds>			
			<max-doublings>0</max-doublings>
		</retry-parameters>
	</queue>	
	<queue>
		<name>loyalty</name>
		<rate>5/m</rate>
